<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="executionContext" factory-bean="peerStarter" factory-method="getExecutionContext"/>
    <bean id="timeProvider" factory-bean="peerStarter" factory-method="getTimeProvider"/>
    <bean id="backlog" factory-bean="peerStarter" factory-method="getBacklog"/>
    <bean id="blockchainProvider" factory-bean="peerStarter" factory-method="getBlockchainProvider"/>
    <bean id="storage" factory-bean="peerStarter" factory-method="getStorage"/>
    <bean id="fork" factory-bean="peerStarter" factory-method="getFork"/>

</beans>